<html>
	<head>
		<title>{{ seo.title }}</title>
		<meta name="keywords" content="{{ seo.keywords }}" />
		<meta name="description" content="{{ seo.description }}" />

		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />

        {% javascripts '@AppBundle/Resources/public/js/ie/html5shiv.js' %}
			<!--[if lte IE 8]><script src="{{ asset_url }}"></script><![endif]-->
        {% endjavascripts %}

        {% stylesheets 'bundles/app/css/main.css' filter='cssrewrite' %}
			<link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}

        {% stylesheets 'bundles/app/css/noscript.css' filter='cssrewrite' %}
			<noscript><link rel="stylesheet" href="{{ asset_url }}" /></noscript>
        {% endstylesheets %}

        {% stylesheets 'bundles/app/css/ie8.css' filter='cssrewrite' %}
			<!--[if lte IE 8]><link rel="stylesheet" href="{{ asset_url }}" /><![endif]-->
        {% endstylesheets %}

	</head>
	<body>

		<!-- Wrapper-->
			<div id="wrapper">

				<!-- Nav -->
					<nav id="nav">
						<a href="#me" class="icon fa-home active"><span>Home</span></a>
						<a href="#work" class="icon fa-folder"><span>Work</span></a>
						<a href="#contact" class="icon fa-envelope"><span>Contact</span></a>
						<a href="#" class="icon fa-twitter"><span>Twitter</span></a>
					</nav>

				<!-- Main -->
					<div id="main">

						<!-- Me -->
							<article id="me" class="panel">
								<header>
									<h1>{{ companyName }}</h1>
									<p>{{ companyTagline }}</p>
								</header>
								<a href="#work" class="jumplink pic">
									<span class="arrow icon fa-chevron-right"><span>See my work</span></span>
									<img src="images/me.jpg" alt="" />
								</a>
							</article>

						<!-- Work -->
							<article id="work" class="panel">
								<header>
									<h2>Work</h2>
								</header>
								<p>
									Phasellus enim sapien, blandit ullamcorper elementum eu, condimentum eu elit.
									Vestibulum ante ipsum primis in faucibus orci luctus et ultrices posuere cubilia
									luctus elit eget interdum.
								</p>
								<section>
									<div class="row">
										<div class="4u 12u$(mobile)">
											<a href="#" class="image fit"><img src="images/pic01.jpg" alt=""></a>
										</div>
										<div class="4u 12u$(mobile)">
											<a href="#" class="image fit"><img src="images/pic02.jpg" alt=""></a>
										</div>
										<div class="4u$ 12u$(mobile)">
											<a href="#" class="image fit"><img src="images/pic03.jpg" alt=""></a>
										</div>
										<div class="4u 12u$(mobile)">
											<a href="#" class="image fit"><img src="images/pic04.jpg" alt=""></a>
										</div>
										<div class="4u 12u$(mobile)">
											<a href="#" class="image fit"><img src="images/pic05.jpg" alt=""></a>
										</div>
										<div class="4u$ 12u$(mobile)">
											<a href="#" class="image fit"><img src="images/pic06.jpg" alt=""></a>
										</div>
										<div class="4u 12u$(mobile)">
											<a href="#" class="image fit"><img src="images/pic07.jpg" alt=""></a>
										</div>
										<div class="4u 12u$(mobile)">
											<a href="#" class="image fit"><img src="images/pic08.jpg" alt=""></a>
										</div>
										<div class="4u$ 12u$(mobile)">
											<a href="#" class="image fit"><img src="images/pic09.jpg" alt=""></a>
										</div>
										<div class="4u 12u$(mobile)">
											<a href="#" class="image fit"><img src="images/pic10.jpg" alt=""></a>
										</div>
										<div class="4u 12u$(mobile)">
											<a href="#" class="image fit"><img src="images/pic11.jpg" alt=""></a>
										</div>
										<div class="4u$ 12u$(mobile)">
											<a href="#" class="image fit"><img src="images/pic12.jpg" alt=""></a>
										</div>
									</div>
								</section>
							</article>

						<!-- Contact -->
							<article id="contact" class="panel">
								<header>
									<h2>Связаться</h2>
								</header>
								<form action="#" method="post">
									<div>
										<div class="row">
											<div class="6u 12u$(mobile)">
												<input type="text" name="name" placeholder="Имя" />
											</div>
											<div class="6u$ 12u$(mobile)">
												<input type="text" name="email" placeholder="Email" />
											</div>
											<div class="12u$">
												<textarea name="message" placeholder="Сообщение" rows="8"></textarea>
											</div>
											<div class="12u$">
												<input type="submit" value="Отправить" />
											</div>
										</div>
									</div>
								</form>
							</article>

					</div>

				<!-- Footer -->
					<div id="footer">
						<ul class="copyright">
							<li>&copy; {{ companyName }} 2017 – {{ year }}.</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
						</ul>
					</div>

			</div>

		<!-- Scripts -->
			{% javascripts
				'@AppBundle/Resources/public/js/jquery.min.js'
            	'@AppBundle/Resources/public/js/skel.min.js'
            	'@AppBundle/Resources/public/js/skel-viewport.min.js'
            	'@AppBundle/Resources/public/js/util.js'
            	'@AppBundle/Resources/public/js/main.js' %}
				<script src="{{ asset_url }}"></script>
			{% endjavascripts %}

			{% javascripts '@AppBundle/Resources/public/js/ie/respond.min.js' %}
				<!--[if lte IE 8]><script src="{{ asset_url }}"></script><![endif]-->
			{% endjavascripts %}
	</body>
</html>